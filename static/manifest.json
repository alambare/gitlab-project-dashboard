{
    "manifest_version": 3,
    "name": "GitLab Project Dashboard",
    "version": "1.0",
    "description": "A browser extension to track the resources consumption in a project.",
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "action": {
        "default_icon": {
            "16": "icons/icon-16.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        },
        "default_title": "Click Me"
    }
}
